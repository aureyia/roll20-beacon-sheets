{
  "name": "@r20sd/ironsworn-starforged",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "sim": "vite --mode simulation",
    "sandbox-server": "vite --port 7620 --mode staging",
    "sandbox": "run-s build-scss sandbox-server",
    "build": "run-s build-scss build-only",
    "preview": "vite preview",
    "test:unit": "vitest --no-watch",
    "test:unit:watch": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:local": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress run --e2e'",
    "test:e2e:open:local": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "build-data": "ts-node ./src/system/parser/parser.ts",
    "build-scss": "sass ./src/system/rolls/rolltemplates/styles/host.scss ./public/host.css --no-source-map --no-charset",
    "watch-scss": "sass --watch ./src/system/rolltemplates/styles/host.scss ./public/host.css --no-source-map --no-charset",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "ci-check": "npm run format && npm run lint && npm run type-check && npm run test:unit && npm run test:e2e:local",
    "cypress:open:local": "CONFIG_FILE=local CYPRESS_ENV=true cypress open",
    "cypress:test:local": "CONFIG_FILE=local CYPRESS_ENV=true cypress run --env TAGS=\"@integration\"",
    "cypress:test:ci": "CONFIG_FILE=local CYPRESS_ENV=true cypress run --env TAGS=\"@integration\"",
    "cypress:report": "allure generate allure-results --clean -o allure-report && allure open allure-report"
  },
  "dependencies": {
    "@effect/platform-browser": "0.63.0",
    "@noble/hashes": "^1.8.0",
    "@paralleldrive/cuid2": "2.2.2",
    "@roll20-official/beacon-sdk": "0.0.31",
    "@vee-validate/zod": "^4.15.0",
    "@vueuse/core": "^13.3.0",
    "@xstate/store": "3.6.2",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "crypto-browserify": "^3.12.1",
    "dataforged": "1.5.3",
    "effect": "3.15.5",
    "handlebars": "4.7.8",
    "jsonpath": "1.1.1",
    "lodash": "4.17.21",
    "lucide-vue-next": "0.511.0",
    "marked": "15.0.11",
    "pinia": "3.0.2",
    "reka-ui": "^2.3.0",
    "tailwind-merge": "3.3.0",
    "tailwindcss-animate": "1.0.7",
    "ts-to-zod": "3.15.0",
    "vee-validate": "^4.15.0",
    "vite-svg-loader": "5.1.0",
    "vue": "3.5.14",
    "vue-component-library": "0.0.8",
    "vue-i18n": "11.1.3",
    "vue-router": "4.5.1",
    "xstate": "5.19.3",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "22.0.1",
    "@cypress/webpack-preprocessor": "6.0.4",
    "@effect/vitest": "0.22.4",
    "@iconify-json/radix-icons": "1.2.2",
    "@iconify/vue": "5.0.0",
    "@rushstack/eslint-patch": "1.11.0",
    "@tailwindcss/postcss": "4.1.7",
    "@testing-library/vue": "8.1.0",
    "@types/jsdom": "21.1.7",
    "@types/jsonpath": "0.2.4",
    "@types/lodash": "4.17.16",
    "@types/node": "22.15.18",
    "@vitejs/plugin-vue": "5.2.4",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.5.0",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "allure-commandline": "2.34.0",
    "allure-cypress": "3.2.2",
    "autoprefixer": "10.4.21",
    "cypress": "14.3.3",
    "cypress-file-upload": "5.0.8",
    "cypress-on-fix": "1.1.0",
    "eslint": "9.27.0",
    "eslint-plugin-cypress": "4.3.0",
    "eslint-plugin-vue": "10.1.0",
    "jsdom": "26.1.0",
    "npm-run-all": "4.1.5",
    "path-browserify": "1.0.1",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "0.6.11",
    "sass": "1.89.0",
    "shadcn-vue": "2.2.0",
    "start-server-and-test": "2.0.12",
    "tailwindcss": "3.4.17",
    "ts-node": "10.9.2",
    "typescript": "~5.8.3",
    "unplugin-vue-components": "28.5.0",
    "vite": "6.3.5",
    "vite-svg-loader": "5.1.0",
    "vitest": "3.1.3",
    "vitest-dom": "0.1.1",
    "vue-tsc": "2.2.10",
    "webpack": "5.99.8"
  },
  "cypress-cucumber-preprocessor": {
    "json": {
      "enabled": true
    },
    "e2e": {
      "stepDefinitions": "support/step_definitions/index.steps.js"
    }
  }
}
