{
  "name": "@r20sd/ironsworn-starforged",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "sim:run": "vite --mode simulation",
    "sim:db:generate": "drizzle-kit generate",
    "sim:db:migrate": "drizzle-kit migrate",
    "sim:db:studio": "drizzle-kit studio",
    "sim:db:start": "turso dev --db-file local.db",
    "sandbox-server": "vite --port 7620 --mode staging",
    "sandbox": "run-s build-scss sandbox-server",
    "build": "run-s build-scss build-only",
    "preview": "vite preview",
    "test:unit": "vitest --no-watch",
    "test:unit:watch": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'playwright'",
    "test:e2e:local": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'playwright'",
    "test:e2e:open:local": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'playwright'",
    "build-only": "vite build",
    "build-data": "ts-node ./src/system/parser/parser.ts",
    "build-scss": "sass ./src/system/rolls/rolltemplates/styles/host.scss ./public/host.css --no-source-map --no-charset",
    "watch-scss": "sass --watch ./src/system/rolltemplates/styles/host.scss ./public/host.css --no-source-map --no-charset",
    "format": "biome format --write ./src",
    "lint": "biome lint --write ./src",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "ci-check": "npm run format && npm run lint && npm run type-check && npm run test:unit && npm run test:e2e:local"
  },
  "dependencies": {
    "@effect/experimental": "0.48.12",
    "@effect/platform-browser": "0.64.11",
    "@paralleldrive/cuid2": "2.2.2",
    "@roll20-official/beacon-sdk": "0.0.31",
    "@vueuse/core": "13.3.0",
    "@xstate/store": "3.6.2",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "effect": "3.16.7",
    "handlebars": "4.7.8",
    "lucide-vue-next": "0.511.0",
    "marked": "15.0.11",
    "reka-ui": "2.3.0",
    "tailwind-merge": "3.3.0",
    "tailwindcss-animate": "1.0.7",
    "vite-svg-loader": "5.1.0",
    "vue": "3.5.14",
    "vue-component-library": "0.0.8",
    "vue-i18n": "11.1.3",
    "vue-router": "4.5.1",
    "xstate": "5.19.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.0",
    "@effect/language-service": "0.20.1",
    "@effect/vitest": "0.22.4",
    "@iconify-json/radix-icons": "1.2.2",
    "@iconify/vue": "5.0.0",
    "@libsql/client": "0.15.9",
    "@noble/hashes": "1.8.0",
    "@playwright/test": "1.52.0",
    "@tailwindcss/vite": "4.1.8",
    "@testing-library/vue": "8.1.0",
    "@types/jsdom": "21.1.7",
    "@types/json-diff": "1.0.3",
    "@types/jsonpath": "0.2.4",
    "@types/lodash": "4.17.16",
    "@types/node": "22.15.18",
    "@vitejs/plugin-vue": "5.2.4",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "dotenv": "16.5.0",
    "drizzle-kit": "0.31.1",
    "drizzle-orm": "0.44.2",
    "jsdom": "26.1.0",
    "json-diff": "1.0.6",
    "npm-run-all": "4.1.5",
    "sass": "1.89.0",
    "shadcn-vue": "2.2.0",
    "start-server-and-test": "2.0.12",
    "tailwindcss": "4.1.8",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "unplugin-vue-components": "28.5.0",
    "vite": "6.3.5",
    "vite-svg-loader": "5.1.0",
    "vitest": "3.1.3",
    "vitest-dom": "0.1.1",
    "vue-tsc": "2.2.10",
    "webpack": "5.99.8"
  }
}
