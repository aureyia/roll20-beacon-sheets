{
  "name": "@r20sd/ironsworn-starforged",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "sandbox-server": "vite --port 7620 --mode staging",
    "sandbox": "run-s build-scss sandbox-server",
    "build": "run-s build-scss build-only",
    "preview": "vite preview",
    "test:unit": "vitest --no-watch",
    "test:unit:watch": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:local": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress run --e2e'",
    "test:e2e:open:local": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "build-scss": "sass ./src/rolltemplates/styles/host.scss ./public/host.css --no-source-map --no-charset",
    "watch-scss": "sass --watch ./src/rolltemplates/styles/host.scss ./public/host.css --no-source-map --no-charset",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "ci-check": "npm run format && npm run lint && npm run type-check && npm run test:unit && npm run test:e2e:local",
    "cypress:open:local": "CONFIG_FILE=local CYPRESS_ENV=true cypress open",
    "cypress:test:local": "CONFIG_FILE=local CYPRESS_ENV=true cypress run --env TAGS=\"@integration\"",
    "cypress:test:ci": "CONFIG_FILE=local CYPRESS_ENV=true cypress run --env TAGS=\"@integration\"",
    "cypress:report": "allure generate allure-results --clean -o allure-report && allure open allure-report"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@roll20-official/beacon-sdk": "^0.0.13",
    "@vee-validate/zod": "^4.13.2",
    "@vueuse/core": "^11.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dataforged": "^1.5.3",
    "handlebars": "^4.7.7",
    "jsonpath": "^1.1.1",
    "lodash": "^4.17.21",
    "lucide-vue-next": "^0.439.0",
    "pinia": "^2.0.32",
    "radix-vue": "^1.9.5",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "vee-validate": "^4.13.2",
    "vite-svg-loader": "^4.0.0",
    "vue": "^3.2.47",
    "vue-component-library": "^0.0.8",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "19.1.0",
    "@cypress/webpack-preprocessor": "5.12.0",
    "@iconify-json/radix-icons": "^1.2.0",
    "@iconify/vue": "^4.1.2",
    "@rushstack/eslint-patch": "^1.2.0",
    "@testing-library/vue": "^7.0.0",
    "@types/jsdom": "^21.1.0",
    "@types/jsonpath": "^0.2.4",
    "@types/lodash": "^4.14.202",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "allure-commandline": "2.29.0",
    "allure-cypress": "2.15.1",
    "autoprefixer": "^10.4.20",
    "cypress": "13.4.0",
    "cypress-file-upload": "5.0.2",
    "cypress-on-fix": "1.0.2",
    "eslint": "^8.34.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "path-browserify": "1.0.1",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "sass": "^1.62.1",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.4.10",
    "typescript": "~4.8.4",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^4.1.4",
    "vite-svg-loader": "^4.0.0",
    "vitest": "^0.31.0",
    "vitest-dom": "^0.1.0",
    "vue-tsc": "^1.2.0",
    "webpack": "5.89.0"
  },
  "cypress-cucumber-preprocessor": {
    "json": {
      "enabled": true
    },
    "e2e": {
      "stepDefinitions": "support/step_definitions/index.steps.js"
    }
  }
}
